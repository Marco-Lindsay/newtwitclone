<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%# link_to 'Follow', user.toggle_follow!(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

